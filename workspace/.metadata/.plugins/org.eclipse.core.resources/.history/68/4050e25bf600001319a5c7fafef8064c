package model;

import java.io.File;

import peakml.IPeak;
import peakml.IPeakSet;

public class Model {
		
	static IdentificationStore pathStore;
	static PeakStore peakStore;

	int[] peakRows, idRows;
	
	public void loadIdentifications(File file){
		pathStore.parseInputFile(file);
	}
	
	public IPeakSet<IPeak> loadPeaks(File file){
		return peakStore.parseInputFile(file);
	}
}